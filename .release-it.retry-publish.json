{
  "increment": false,
  "git": {
    "changelog": false,
    "commit": false,
    "tag": false,
    "push": false,
    "requireCleanWorkingDir": false,
    "requireBranch": false
  },
  "npm": {
    "publish": true,
    "skipChecks": true,
    "publishArgs": ["--provenance", "--access", "public"]
  },
  "github": {
    "release": true,
    "update": true,
    "releaseNotes": "pnpm tsx scripts/extract-changelog.ts ${version}"
  }
}
